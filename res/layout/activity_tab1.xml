<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="2dp"
android:paddingLeft="2dp"
android:paddingRight="2dp"
android:paddingTop="2dp"
tools:context="com.example.myfirstapp.Tab1Activity" >

<LinearLayout 
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical" >
<ScrollView
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:scrollbars="none"
android:layout_weight="1"> 
<TableLayout 
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:stretchColumns="1"
android:textSize="4pt"
>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/row1"
android:text="See more Details on Zillow"
android:layout_height="wrap_content"

/> 
</TableRow>

<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/row2"
android:layout_span="2"
android:layout_height="wrap_content"
android:textColorLink="#08088A"

/> 
</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/proptyp"
android:text="Property Type:"
android:layout_height="wrap_content"

/>
<TextView
android:id="@+id/proptyp1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>

<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/yrbt"
android:text="Year Built:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/yrbt1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>

<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/ls"
android:text="Lot Size:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/ls1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/fa"
android:text="Finished Area:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/fa1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>


<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/ba:"
android:text="Bathrooms:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/ba1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/be"
android:text="Bedrooms:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/be1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/taxyr"
android:text="Tax Assessment Year:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/taxyr1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/taxass"
android:text="Tax Assessment :"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/taxass1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/lastsoldpr"
android:text="Last Sold Price :"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/lastsoldpr1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>

<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/lastsolddt"
android:text="Last Sold Date:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/lastsolddt1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/zestprop"

android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/zestpropval1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/thirtydaysoverallchange"
android:text="30 Days Overall Change:"
android:layout_height="wrap_content"
/>
<LinearLayout 
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="horizontal"
android:layout_gravity="right" >
<ImageView 
android:id="@+id/oc1"
android:layout_alignParentRight="true"
android:layout_width="wrap_content"
android:layout_height="wrap_content"

android:paddingTop="6dp"/> 
<TextView
android:id="@+id/thirtydaysoverallchange1"

android:layout_width="wrap_content"
android:layout_height="wrap_content"
/>

</LinearLayout>
</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/alltimeprop"
android:text="All Time Property Range:"
android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/alltimeproplowhigh"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>

<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/restprop"

android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/restpropval1"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#ffffff">
<TextView
android:id="@+id/thirtydaysrentchange"
android:text="30 Days Rent Change:"
android:layout_height="wrap_content"
/>
<LinearLayout 
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="horizontal"
android:layout_gravity="right" >
<ImageView 
android:id="@+id/rent_pic"
android:layout_alignParentRight="true"
android:layout_width="wrap_content"
android:layout_height="wrap_content"

android:paddingTop="6dp"/> 
<TextView
android:id="@+id/thirtydaysrentchange1"

android:layout_width="wrap_content"
android:layout_height="wrap_content"
/>

</LinearLayout>
</TableRow>
<TableRow android:layout_height="wrap_content"
android:paddingTop="6dp"
android:paddingBottom="6dp"
android:background="#F7F7FF">
<TextView
android:id="@+id/alltimerent"
android:text="All Time Rent Range:"

android:layout_height="wrap_content"
/>
<TextView
android:id="@+id/alltimerentlowhigh"
android:layout_gravity="right"
android:layout_height="wrap_content"
/>

</TableRow>

</TableLayout>
</ScrollView>
<TextView
android:id="@+id/zillow_tnc"
android:background="#ffffff"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:gravity="center"
android:textColorLink="#000000"
android:text="\u00A9 Zillow, Inc., 2006-2014."
/>
<TextView
android:id="@+id/zillow_tnc2"
android:background="#ffffff"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:gravity="center"
android:textColorLink="#000000"
/>
<TextView
android:id="@+id/zillow_tnc3"
android:background="#ffffff"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:gravity="center"
android:textColorLink="#000000"
/>
</LinearLayout>

</RelativeLayout>